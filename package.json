{
  "name": "agies",
  "version": "1.0.0",
  "description": "Agies - The Maze Vault Password Manager with revolutionary Chakravyuham security system",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec ts-node src/backend/index.ts",
    "dev:frontend": "vite",
    "dev:tauri": "tauri dev",
    "build": "echo 'Build disabled for Vercel deployment'",
    "build:js": "babel public/js --out-dir dist/js --copy-files",
    "build:backend": "tsc",
    "build:frontend": "vite build",
    "build:tauri": "tauri build",
    "watch:js": "babel public/js --out-dir dist/js --copy-files --watch",
    "dev:js": "npm run watch:js",
    "start": "node dist/index.js",
    "start:desktop": "tauri",
    "test": "jest",
    "test:security": "npm run test -- --testPathPattern=security",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "agies:launch": "npm run build && npm run start:desktop"
  },
  "keywords": [
    "agies",
    "password-manager",
    "security",
    "vault",
    "encryption",
    "cybersecurity",
    "chakravyuham",
    "maze-security",
    "zero-knowledge",
    "ai-security",
    "dark-web-monitoring"
  ],
  "author": "Agies Team",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.15.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.541.0",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.1",
    "nodemailer": "^6.9.7",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^2.4.2",
    "react-router-dom": "^6.20.1",
    "redis": "^4.7.1",
    "sharp": "^0.32.6",
    "speakeasy": "^2.0.0",
    "stripe": "^18.4.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
